    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "formik": "^2.2.9",
    "next": "12.1.6",
    "nextjs-google-analytics": "^2.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.0",
    "react": "18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-datepicker": "^4.8.0",
